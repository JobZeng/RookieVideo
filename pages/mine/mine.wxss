/* pages/mine.wxss */
.userinfo {
  height: 300rpx;
  background-color: #f5f5f5;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-evenly;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #333;
}

.content {
  height: 600rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-evenly;
}